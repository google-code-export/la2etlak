<div>
<%= form_for (@feed, :url => {:action => 'create', :idinterest => params[:id]}) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= f.text_field :link %>
  </div>

  <div class="actions">
    <%= f.submit "Add RSS feed"  , class:"button round"%>
  </div>
<% end %>
  </div>



